<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        xmlns:ext="http://schemas.haulmont.com/cuba/window-ext.xsd"
        extends="com/company/itpearls/web/screens/iteractionlist/iteraction-list-browse.xml"
        messagesPack="com.company.itpearls.web.screens.iteractionlist">
    <data>
        <collection id="iteractionListsDc" class="com.company.itpearls.entity.IteractionList">
            <view/>
            <loader id="iteractionListsDl" cacheable="true">
                <query><![CDATA[select e
                                from itpearls_IteractionList e
                                where (e.iteractionType.interactionStatus is not null) and
                                    not (e.iteractionType.interactionStatus = 0)
                                order by e.numberIteraction desc]]>
                </query>
            </loader>
        </collection>
    </data>
    <layout/>
</window>
